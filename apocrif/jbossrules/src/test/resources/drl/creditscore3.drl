package creditscore

import creditscore.CreditScore

rule "Credit_Score_Adjustments_0"
    no-loop true
when 
    cs : CreditScore( programGroup=="ACMEPowerBuyerGroup",
                      lienType=="FIRST_TD; SECOND_TD",
                      division=="Wholesale",
                      score >= 500,
                      score <= 579 )
then
    cs.setScore(cs.getScore() + 1);
    modify(cs);
end
